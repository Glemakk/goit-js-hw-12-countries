{{#each this}}
<div class="coutryCard">
    <div>
        <h2>{{name}}</h2>
        <ul>
            <li class="capital">
                <span>Capital: {{capital}}</span>
            </li>
            <li class="population">
                <span>Population: {{population}}</span>
            </li>
            <li class="languages">
                Languages:
                <ul>
                    {{#each languages}}
                    <li>
                        {{name}}
                    </li>
                    {{/each}}
                </ul>
        
            </li>
        </ul>
    </div>
    <div class="flagImg">
        <img src="{{flag}}" alt="{{name}} flag" width="300">
    </div>
</div>
{{/each}}